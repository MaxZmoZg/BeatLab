@model IEnumerable<BeatLab.Models.Entities.Genere_Of_Music>

@{
    ViewBag.Title = "Жанр музыки";
}

<h2>Жанр музыки</h2>

<p>
    @Html.ActionLink("Добавить новый", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Название жанров музыки
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name_Gener_of_music)
        </td>
        <td>
            @Html.ActionLink("Редактировать", "Edit", new { id=item.ID_Genere_Of_Music }) |
            @Html.ActionLink("Удалить", "Delete", new { id=item.ID_Genere_Of_Music })
        </td>
    </tr>
}

</table>
